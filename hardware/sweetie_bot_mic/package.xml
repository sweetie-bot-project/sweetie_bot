<package format="2">
  <name>sweetie_bot_mic</name>
  <version>0.0.0</version>
  <description>The sweetie_bot_mic package</description>

  <!-- One maintainer tag required, multiple allowed, one person per tag -->
  <maintainer email="goncharovoi@yandex.ru">disRecord</maintainer>
  <maintainer email="fsdfur@gmail.com">mutronics</maintainer>

  <!-- One license tag required, multiple allowed, one license per tag -->
  <license>BSD</license>

  <!-- Url tags are optional, but multiple are allowed, one per tag -->
  <url type="website">https://sweetiebot.net</url>
  <url type="website">https://gitlab.com/sweetie-bot/sweetie_doc/wikis</url>

  <!-- Author tags are optional, multiple are allowed, one per tag -->
  <author email="goncharovoi@yandex.ru">disRecord</author>

  <!-- The *depend tags are used to specify dependencies -->
  <buildtool_depend>catkin</buildtool_depend>
  <!-- System dependencies -->
  <exec_depend>python3-gi</exec_depend>
  <exec_depend>python3-numpy</exec_depend>
  <exec_depend>python3-pykdl</exec_depend>
  <!-- Non-standard system dependencies -->
  <exec_depend>python3-pyroomacoustics</exec_depend>
  <!-- ROS dependencies -->
  <depend>dynamic_reconfigure</depend>
  <exec_depend>rospy</exec_depend>
  <exec_depend>tf</exec_depend>
  <!-- ROS messages dependencies -->
  <exec_depend>visualization_msgs</exec_depend>
  <exec_depend>std_msgs</exec_depend>
  <exec_depend>geometry_msgs</exec_depend>
  <exec_depend>std_srvs</exec_depend>
  <!-- SweetieBot messages dependencies -->
  <exec_depend>sweetie_bot_text_msgs</exec_depend>

  <!-- The export tag contains other, unspecified, tags -->
  <export>
    <!-- Other tools can request additional information be placed here -->
  </export>
</package>
