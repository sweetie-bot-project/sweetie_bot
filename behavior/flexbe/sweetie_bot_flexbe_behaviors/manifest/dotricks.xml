<?xml version="1.0" encoding="UTF-8"?>

<behavior name="DoTricks">

	<executable package_path="sweetie_bot_flexbe_behaviors.dotricks_sm" class="DoTricksSM" />
	<tagstring>SweetieBot, good, tricks</tagstring>
	<author>disRecord</author>
	<date>Tue Mar 28 2017</date>
	<description>
		Basic interaction with people behavior.
	</description>


	<!-- Contained Behaviors -->
	<contains name="Greeting" />
	<contains name="Cheer" />
	<contains name="Play" />
	<contains name="Bad" />

	<!-- Available Parameters -->
	<params>

		<param type="boolean" name="be_evil" default="False" label="be_evil" hint="Sets the be_evil" />

		<param type="numeric" name="wait_time" default="1" label="wait_time" hint="Sets the wait_time">
			<min value="0" />
			<max value="100" />
		</param>

	</params>


</behavior>